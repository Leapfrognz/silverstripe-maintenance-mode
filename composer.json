{
    "name": "wedevelopnl/silverstripe-maintenance-mode",
    "description": "Maintenance mode for Silverstripe",
    "type": "silverstripe-vendormodule",
    "repositories": [
        {
            "url": "https://github.com/wedevelopnl/silverstripe-maintenance-mode.git",
            "type": "git"
        }
    ],
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "silverstripe/framework": "^4@stable",
        "silverstripe/errorpage": "~1.11.0@stable"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\MaintenanceMode\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        },
        "suggest": {
            "wedevelopnl/silverstripe-elemental-grid": "Enhances the error pages by enabeling a grid on them."
        }
    }
}
